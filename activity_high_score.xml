<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_high_score"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.example.toanncth.quizapp.HighScoreActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        app:srcCompat="@drawable/highscore"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="Khoa Học Xã hội"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView1" />
        <TextView
            android:text="TextView"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView2" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="Toán Học"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView3" />
        <TextView
            android:text="TextView"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView4" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="Văn Học - Lịch Sử"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView5" />
        <TextView
            android:text="TextView"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:id="@+id/textView6" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="Địa Lý"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
