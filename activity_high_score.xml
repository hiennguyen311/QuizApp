        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="Câu Đố Cười"
            android:textStyle="bold"
            android:layout_width="150dp"
            android:layout_height="25dp"
